/* Component options */
/* modals */
.modal .widget,
.modal .modal-body,
.modal .modal-content,
.modal .modal-dialog {
    background-color: #fff !important;
}
.modal .modal-content {
    border-width: 0;
    border-color: #fff;
}
/* affected classes for border-radius */
.modal .modal-content,
.modal .dialog,
.modal {
    border-radius: 0;
}
.modal-content .widget.widget-tabs.widget-tabs-responsive > .widget-head,
.modal-body .melis-nav-tabs-box {
    border-top-left-radius: 0;
    border-top-right-radius: 0;
}
/* end affected classes for broder-radius */
/** options_modal_nav_tabs_color */
.modal-content .widget .widget-head {
    background-color: #e61c23;
}
/** options_modal_nav_tabs_active_bg_color */
.modal-content .nav-tabs > li.active > a {
    background-color: #fff;
}
/** options_modal_nav_tabs_border_right_color, options_modal_nav_tabs_border_right_width */
.modal-content .widget.widget-tabs.widget-tabs-responsive > .widget-head ul li {
    border-right-color: #ce5459;
    border-right-width: 1px;
}
/** options_modal_nav_tabs_text_color */
.modal-content .widget.widget-tabs-double > .widget-head ul li:not(.active) a.glyphicons i:before, 
.modal-content .widget.widget-tabs-double .widget-head ul li a.glyphicons i:before,
.modal-content .widget.widget-tabs-double > .widget-head ul li:not(.active) a.glyphicons,
.modal-content .widget.widget-tabs-double .widget-head ul li a.glyphicons {
    color: #fff;
    font-size: 14px;
}
/** options_modal_content_text_color, options_modal_content_form_group_label */
.modal-content .span,
.modal-content .form-group span,
.modal-content .form-group label,
.modal-content p {
    color: #686868;;
}
/** options_modal_content_text_font_size */
.modal-content .span,
.modal-content .form-group span,
.modal-content p {
    font-size: 14px;
}
/** options_modal_content_form_group_label_font_size */
.modal-content .form-group label {
    font-size: 13px;
}
/** options_modal_content_text_font_styles */
.modal-content .span,
.modal-content .form-group span,
.modal-content p {
    font-style: normal;
    font-weight: normal;
    text-transform: inherit;
    text-decoration: inherit;
}
/** options_modal_content_close_btn_bg_color, options_modal_content_close_btn_border_color, options_modal_content_close_btn_text_color */
.modal-content .btn-danger {
    background-color: #fff;
    border-color: #bd362f;
    color: #bd362f;
}
/** options_modal_content_close_btn_active_bg_color, options_modal_content_close_btn_active_text_color */
.modal-content .btn-danger:hover {
    background-color: #bd362f;
    color: #fff;
}