                            change: function(hex, opacity) {
                                var log;
                                    try {
                                        log = hex ? hex : 'transparent';

                                        if ( opacity ) 
                                            log += ', ' + opacity;
                                        
                                        if ( $this.closest(".osta_color_code").find(".minicolor-hex").length ) {
                                            $this.closest(".osta_color_code").find(".minicolor-hex").val(hex);
