        <script type="text/javascript">
            (function($) {
                //$(".osta_color_code").colorpicker({format: 'hex',useAlpha: false});
                $(".minicolor").each(function() {
                    var $this = $(this);
                    $this.minicolors({
                        control: $this.attr("data-control") || 'hue',
                        defaultValue: $this.attr("data-defaultValue") || '',
                        format: $this.attr('data-format') || 'hex',
                        keywords: $this.attr('data-keywords') || '',
                        inline: $this.attr('data-inline') === 'true',
                        letterCase: $this.attr('data-letterCase') || 'lowercase',
                        opacity: $this.attr('data-opacity') || true,
                        position: $this.attr('data-position') || 'bottom',
                        swatches: $this.attr('data-swatches') ? $this.attr('data-swatches').split('|') : [],
                        change: function(hex, opacity) {
                            var log;
                            try {
                                /* log = hex ? hex : 'transparent';
                                
                                if (opacity)
                                    log += ', ' + opacity; */

                                if ($this.closest(".osta_color_code").find(".minicolor-hex").length) {
                                    if (opacity === '0.00') {
                                        $this.closest(".osta_color_code").find(".minicolor-hex").val('transparent');
                                    } else {
                                        $this.closest(".osta_color_code").find(".minicolor-hex").val(hex);
                                    }
                                }
                            } catch (e) {
                                console.error(e);
                            }
                        },
                        theme: 'default'
                    });
                });

                $(".platform-scheme .minicolor-hex").each(function(i, v) {
                    var $this = $(v);
                    $this.on("click", function() {
                        // .minicolors-focus, .minicolors-panel style="display: block;"
                        $this.parent(".input-group").find(".minicolors .minicolors-swatch").trigger("click");
                    });
                });

                $("input:file").filestyle({
                    buttonName: "btn-primary",
                    buttonText: '<?= $this->translate('tr_meliscore_platform_scheme_file_button'); ?>'
                });
                $("input#sidebar_header_logo:file").filestyle({
                    buttonName: "btn-primary",
                    input: false,
                    buttonText: '<?= $this->translate('tr_meliscore_platform_scheme_file_button'); ?>'
                });
            })(jQuery);
        </script>
